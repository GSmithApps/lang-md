{
	"name": "rust-md",
	"description": "Rust md - a rustmd previewer",
	"displayName": "Rust MD",
	"version": "0.0.1",
	"publisher": "Grant Smith",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/GSmithApps/webview-sample"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onWebviewPanel:rustMD"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rustmd.start",
				"title": "Open RustMd preview",
				"category": "Rust MD"
			}
		],
		"languages": [{
			"id": "hin",
			"aliases": ["Hindent", "hin"],
			"extensions": [".rustmd"],
			"configuration": "./rusdmd-configuration.json"
		  }],
		  "grammars": [{
			"language": "hin",
			"scopeName": "source.hin",
			"path": "./syntaxes/hin.tmLanguage.json"
		  }]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.4.2"
	},
	"dependencies": {
		"highlight.js": "^11.9.0"
	}
}
